@model IDictionary<string, string>
@using Sitecore.Mvc.Analytics.Extensions

@Html.Sitecore().VisitorIdentification()



@foreach (var langu in @Model)
{
  if (@langu.Key == "French")
  {
    <a id="lnkFrench" class="langOption fr" href="@langu.Value" aria-label="Français">fr</a>
  }
  else
  {
    <a id="lnkEnglish" class="langOption en" href="@langu.Value" aria-label="English">en</a>
  }
}