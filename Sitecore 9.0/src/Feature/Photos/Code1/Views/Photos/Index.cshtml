@model Chartwell.Feature.Photo.Model.PhotoGalleryModel
@using Sitecore.Mvc.Analytics.Extensions

@Html.Sitecore().VisitorIdentification()



@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Data

<div>
  <h1 class="resName">@Html.Sitecore().Field("Property Name", @Model.InnerItem) </h1>

  <address class="resAddress">
    @Model.PropertyFormattedAddress

  </address>
  <blockquote class="resQuote blockquote">@Html.Sitecore().Field("Property Tag Line", @Model.InnerItem) </blockquote>
  <div>

    @foreach (var imageUrl in @Model.ImageUrls)
    {



      <a class="fancybox-thumb" rel="fancybox-thumb" data-fancybox="images" data-type="image"
         href="@imageUrl">
        <img src="@imageUrl?mw=159">
      </a>

    }


  </div>
</div>