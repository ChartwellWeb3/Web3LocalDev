@model Chartwell.Feature.VirtualTour.Models.VirtualTourModel
@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Data
@using Sitecore.Mvc.Analytics.Extensions

@Html.Sitecore().VisitorIdentification()


<h1 class="resName">@Html.Sitecore().Field("Property Name", @Model.InnerItem) </h1>
<address class="resAddress">
  @Model.PropertyFormattedAddress

</address>
<blockquote class="resQuote blockquote">@Model.PropertyTagLine</blockquote>




<div class="embed-responsive embed-responsive-16by9"> <iframe class="embed-responsive-item" src='https://www.youtube.com/embed/@Model.VideoLink?rel=0&iv_load_policy=3&enablejsapi=1' allowFullScreen=""></iframe> </div>